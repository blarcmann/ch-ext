{
  "manifest_version": 2,
  "name": "stash OG",
  "version": "1.0",
  "description": "when it comes to money, let the OGs handle it",
  "icons": {
    "128": "icon128.png",
    "48": "icon48.png",
    "16": "icon16.png"
  },
  "options_page": "options.html",
  "background": {
    "scripts": ["eventPage.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "favicon.png",
    "default_popup": "popup.html",
    "default_title": "A popup bitch" 
  },
  "permissions": ["storage", "notifications", "contextMenus"]
}
